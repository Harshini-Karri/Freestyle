{
  "_version": "1.76.0",
  "sap.app": {
    "id": "com.ust.purchaseorders",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "319848d1-1c37-4905-b21e-8c4061f15f1d"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/my/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.ust.purchaseorders.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.ust.purchaseorders.view",
        "async": true,
        "viewPath": "com.ust.purchaseorders.view"
      },
      "routes": [
        {
          "name": "RouteMainview",
          "pattern": ":?query:",
          "target": [
            "Targetcard"
          ]
        },
        { "pattern": "card",       
            "name": "RouteCard",  
               "target": ["Card"] 
              },
        { 
          "pattern": "Vendor",   
            "name": "Vendor",  
             "target": ["Vendor"]
             },
        { 
          "pattern": "Material",
             "name": "Material", 
             "target": ["Material"] 
            },
        {
           "pattern": "poheader",
              "name": "POHeader",
               "target": ["POHeader"] 
              },
        { 
          "pattern": "POItem",    
           "name": "POItem", 
           "target":["POItem"]
           },
        { 
          "pattern": "gr",       
            "name": "GR",    
               "target": ["GR"] 
              },
        { 
          "pattern": "invoice",   
           "name": "Invoice",
             "target": ["Invoice"]
             }
      ],
      
      "targets": {
        "Targetcard": {
          "id":"card",
          "name": "card"
        },
        "Vendor": {
          "viewName": "Vendor"
        },
        "Material": {
          "viewName": "Material"
        },
        "POHeader": {
          "viewName": "POHeader"
        },
        "POItem": {
          "viewName": "POItem"
        },
        "GR": {
          "viewName": "GR"
        },
        "Invoice": {
          "viewName": "Invoice"
        }
      }
    },
    "rootView": {
      "viewName": "com.ust.purchaseorders.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
